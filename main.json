{
  "manifest_version": 3,
  "name": "Web Time Tracker",
  "version": "1.0",
  "description": "Track time spent on websites",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "host_permissions": ["<all_urls>"]
}
